.help-block {
  margin-top: 25px;
  font-size: 0.9rem;
  color: #ff9999;
}

/* ====== Style pour les titres dans le jeu (ex: "Wake up $name !!") ====== */
.Titrenom {
  /* Police pixelisée pour les titres */
  font-family: 'Press Start 2P', monospace;
  font-size: 3rem;
  color:#4682B4;
  margin-bottom: 20px;
  animation: flicker 2s infinite;
}

/* ====== Style pour les longs blocs de texte ====== */
.Long {
  max-height: 100%;
  font-size: 0.8em;
}

/* ====== Style spécifique pour la carte de Victoire (Bleu) ====== */
.victory-card {
    border: 3px solid #00c0ff; /* Bordure bleue */
    box-shadow: 0 0 25px #00c0ff; /* Ombre bleue */
    animation: pulseBorderBlue 3s infinite alternate; /* Nouvelle animation pour le bleu */
    background: rgba(0, 0, 0, 0.85); /* Fond légèrement plus opaque */
}

/* Titres, sous-titres et argent en bleu pour la victoire */
.victory-card .title,
.victory-card .subtle,
.victory-card .big-money {
    color: #4682B4; 
    text-shadow: 0 0 10px #00c0ff, 0 0 30px #00c0ff; /* Ombre de texte bleue */
}

/* Texte de paragraphe de victoire en bleu clair */
.victory-card p {
    color: #E0FFFF;
}

/* Liens de victoire */
.victory-card .actions tw-link {
    border-color: #00c0ff;
    color: #00c0ff; 
}

.victory-card .actions tw-link:hover {
    background: #00c0ff; 
    color: #000; 
    box-shadow: 0 0 10px #00c0ff, 0 0 20px #00c0ff;
}


/* ====== Animations ====== */

/* Scintillement pour les titres */
@keyframes flicker {
  0%, 19%, 21%, 23%, 25%, 54%, 56%, 100% {
    opacity: 1;
  }
  20%, 24%, 55% {
    opacity: 0.3;
  }
}

/* Pulsation de la bordure (rouge pour défaite) */
@keyframes pulseBorder {
  from {
    box-shadow: 0 0 10px #ff0000;
  }
  to {
    box-shadow: 0 0 30px #ff0000;
  }
}

/* Pulsation de la bordure (bleu pour victoire) */
@keyframes pulseBorderBlue {
  from {
    box-shadow: 0 0 10px #00c0ff;
  }
  to {
    box-shadow: 0 0 30px #00c0ff;
  }
}

/* Fondu à l'apparition */
.fade-in {
  animation: fadeIn 1.5s ease forwards;
}

@keyframes fadeIn {
  from {
    opacity: 0;
    transform: scale(0.95);
  }
  to {
    opacity: 1;
    transform: scale(1);
  }
}